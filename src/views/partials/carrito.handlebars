<!-- carrito.handlebars -->

{{#if cartData}}
    <p>ID del Carrito: {{cartData._doc._id}}</p>
    <ul>
        {{#each cartData._doc.products}}
            <li>
                <strong>Producto ID:</strong> {{this.product._id}} |
                <strong>Cantidad:</strong> {{this.quantity}} |
                <strong>Item ID:</strong> {{this._id}}
            </li>
        {{/each}}
    </ul>
{{else}}
    <p>No se encontraron datos del carrito.</p>
{{/if}}



<script>   
    console.log("Datos del carrito:", {{{jsonStringify cartData}}});
</script>
